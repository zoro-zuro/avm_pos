{
  "name": "my-pos-app",
  "private": true,
  "version": "0.0.3",
  "type": "module",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "vite",
    "check": "node check-build-errors.js",
    "preview": "vite preview",

    "rebuild:electron": "pnpm exec electron-rebuild -f",

    "build": "node check-build-errors.js && tsc && vite build && pnpm rebuild:electron",
    "build:release": "node check-build-errors.js && tsc && vite build && pnpm rebuild:electron && electron-builder",

    "rebuild:node": "pnpm rebuild better-sqlite3",
    "db:generate": "pnpm rebuild:node && drizzle-kit generate",
    "db:push": "pnpm rebuild:node && drizzle-kit push",
    "db:studio": "pnpm rebuild:node && drizzle-kit studio"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "bcrypt": "^6.0.0",
    "better-sqlite3": "^12.5.0",
    "bindings": "^1.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.45.1",
    "electron-pos-printer": "^1.3.8",
    "file-uri-to-path": "^2.0.0",
    "lucide-react": "^0.562.0",
    "node-gyp-build": "^4.8.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.11.0",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@electron/rebuild": "^4.0.2",
    "@types/bcrypt": "^6.0.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.23",
    "drizzle-kit": "^0.31.8",
    "electron": "^39.2.7",
    "electron-builder": "^24.13.3",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.2.2",
    "vite": "^5.1.6",
    "vite-plugin-electron": "^0.28.6",
    "vite-plugin-electron-renderer": "^0.14.5"
  }
}
